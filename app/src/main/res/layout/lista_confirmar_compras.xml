<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv9238"
        android:layout_width="64dp"
        android:layout_height="27dp"
        android:layout_marginStart="8dp"
        android:fontFamily="@font/kionaregular"
        android:text="Nombre: "
        android:textColor="@color/browser_actions_bg_grey"
        android:textSize="7sp"
        android:textStyle="italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_confirmar_compra_nombre"
        android:layout_width="0dp"
        android:layout_height="37dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/kionaregular"
        android:text="TextView"
        android:textColor="@color/browser_actions_bg_grey"
        android:textSize="7sp"
        android:textStyle="italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv9238" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="12dp"
        android:fontFamily="@font/kionaregular"
        android:text="Cantidad: "
        android:textColor="@color/browser_actions_bg_grey"
        android:textSize="7sp"
        android:textStyle="italic"
        app:layout_constraintStart_toEndOf="@+id/tv9238"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_compra_confirmacion_cantidad"
        android:layout_width="0dp"
        android:layout_height="37dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/kionaregular"
        android:text="TextView"
        android:textColor="@color/browser_actions_bg_grey"
        android:textSize="7sp"
        android:textStyle="italic"
        app:layout_constraintStart_toEndOf="@+id/tv_confirmar_compra_nombre"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="0dp"
        android:layout_height="23dp"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/kionaregular"
        android:text="Precio: "
        android:textColor="@color/browser_actions_bg_grey"
        android:textSize="7sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_compra_confirmacion_precio"
        android:layout_width="163dp"
        android:layout_height="37dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/kionaregular"
        android:text="TextView"
        android:textColor="@color/browser_actions_bg_grey"
        android:textSize="7sp"
        android:textStyle="italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.455"
        app:layout_constraintStart_toEndOf="@+id/tv_compra_confirmacion_cantidad"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

</android.support.constraint.ConstraintLayout>